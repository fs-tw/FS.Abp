<Project Sdk="Microsoft.NET.Sdk">

  <Import Project="..\..\common.props" />

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>WB</RootNamespace>
    <GenerateEmbeddedFilesManifest>true</GenerateEmbeddedFilesManifest>
  </PropertyGroup>

  <ItemGroup>
    
    <PackageReference Include="Volo.Abp.Identity.Domain.Shared" Version="4.2.1" />
    <PackageReference Include="Volo.Abp.IdentityServer.Domain.Shared" Version="4.2.1" />
    <PackageReference Include="Volo.Abp.BackgroundJobs.Domain.Shared" Version="4.2.1" />
    <PackageReference Include="Volo.Abp.AuditLogging.Domain.Shared" Version="4.2.1" />
    <PackageReference Include="Volo.Abp.TenantManagement.Domain.Shared" Version="4.2.1" />
    <PackageReference Include="Volo.Abp.FeatureManagement.Domain.Shared" Version="4.2.1" />
    <PackageReference Include="Volo.Abp.PermissionManagement.Domain.Shared" Version="4.2.1" />
    <PackageReference Include="Volo.Abp.SettingManagement.Domain.Shared" Version="4.2.1" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Localization\WB\*.json" />
    <Content Remove="Localization\WB\*.json" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.FileProviders.Embedded" Version="5.0.*" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\modules\FS.Wbm\src\FS.Wbm.Domain.Shared\FS.Wbm.Domain.Shared.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="DataModel\WB.cs">
      <AutoGen>True</AutoGen>
      <DependentUpon>WB.efml</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <None Update="DataModel\WB.Diagram1.view">
      <AutoGen>True</AutoGen>
      <DependentUpon>WB.efml</DependentUpon>
    </None>
    <None Update="DataModel\WB.edps">
      <AutoGen>True</AutoGen>
      <DependentUpon>WB.efml</DependentUpon>
    </None>
    <None Update="DataModel\WB.efml">
      <Generator>DevartEFCoreGenerator</Generator>
      <LastGenOutput>WB.info</LastGenOutput>
    </None>
    <None Update="DataModel\WB.info">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>WB.efml</DependentUpon>
    </None>
  </ItemGroup>

</Project>
