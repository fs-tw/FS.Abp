{
  "version": 1,
  "projects": {
    "account": {
      "projectType": "library",
      "root": "libs/account",
      "sourceRoot": "libs/account/src",
      "prefix": "fs",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/account/tsconfig.lib.json",
            "project": "libs/account/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/account/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/account/tsconfig.lib.json",
              "libs/account/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/account/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/account"
          ],
          "options": {
            "jestConfig": "libs/account/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "theme-unify": {
      "projectType": "library",
      "root": "libs/theme-unify",
      "sourceRoot": "libs/theme-unify/src",
      "prefix": "fs",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "tsconfig.lib.json",
            "project": "libs/theme-unify/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/theme-unify/src/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/theme-unify/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "schematics": {}
    },
    "theme-alain-ms": {
      "projectType": "library",
      "root": "libs/theme-alain-ms",
      "sourceRoot": "libs/theme-alain-ms/src",
      "prefix": "fs",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/theme-alain-ms/tsconfig.lib.json",
            "project": "libs/theme-alain-ms/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/theme-alain-ms/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/theme-alain-ms/tsconfig.lib.json",
              "libs/theme-alain-ms/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/theme-alain-ms/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/theme-alain-ms"
          ],
          "options": {
            "jestConfig": "libs/theme-alain-ms/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "emailing": {
      "projectType": "library",
      "root": "libs/emailing",
      "sourceRoot": "libs/emailing/src",
      "prefix": "fs",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/emailing/tsconfig.lib.json",
            "project": "libs/emailing/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/emailing/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/emailing/tsconfig.lib.json",
              "libs/emailing/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/emailing/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/emailing"
          ],
          "options": {
            "jestConfig": "libs/emailing/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "tslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "css"
    }
  },
  "defaultProject": ""
}
