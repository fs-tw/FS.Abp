{
  "production": false,
  "hmr": false,
  "application": {
    "baseUrl":"http://localhost:4200",
    "name": "<%= title %>"
  },
  "oAuthConfig": {
    "issuer": "https://localhost:<%= port %>",
    "clientId": "<%= applicationName %>_App",
    "responseType": "password",
    "requireHttps":false
  },
  "apis": {
    "default": {
      "url": "https://localhost:<%= port %>",
      "rootNamespace": ""
    }
  }
}