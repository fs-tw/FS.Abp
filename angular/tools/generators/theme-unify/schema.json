{
  "cli": "ng",
  "id": "theme-unify",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "NG專案名稱(projectname-public-app)"
    },
    "cshapBacknedPort": {
      "type": "number",
      "description": "後台啟動Port number: 4xxxx"
    },
    "abpApplicationName": {
      "type": "string",
      "description": "ABP WebSite模組名稱:NKUST,IIC..."
    },
    "title": {
      "type": "string",
      "description": "html title tag(預設:豐碩資訊)"
    },
    "isIncludeSourceCode":{
      "type":"boolean",
      "default": true,
      "description": "是否包含原始碼(說明:目前版本只能是True)"
    }
  },
  "required": ["name","cshapBacknedPort","abpApplicationName"]
}
