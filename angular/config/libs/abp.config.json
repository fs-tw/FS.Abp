{
    "order": 200,
    "angular": {
        "abp": {
            "projectType": "library",
            "root": "libs/abp",
            "sourceRoot": "libs/abp/src",
            "prefix": "fs-tw",
            "architect": {
                "build": {
                    "builder": "@nrwl/angular:package",
                    "outputs": [
                        "dist/libs/abp"
                    ],
                    "options": {
                        "project": "libs/abp/ng-package.json"
                    },
                    "configurations": {
                        "production": {
                            "tsConfig": "libs/abp/tsconfig.lib.prod.json"
                        },
                        "development": {
                            "tsConfig": "libs/abp/tsconfig.lib.json"
                        }
                    },
                    "defaultConfiguration": "production"
                }
            },
            "tags": []
        }
    },
    "dependencies": {
        "@abp/ng.account": "~4.4.4",
        "@abp/ng.components": "~4.4.4",
        "@abp/ng.core": "~4.4.4",
        "@abp/ng.identity": "~4.4.4",
        "@abp/ng.setting-management": "~4.4.4",
        "@abp/ng.tenant-management": "~4.4.4",
        "@abp/ng.theme.basic": "~4.4.4",
        "@abp/ng.theme.shared": "~4.4.4",
        "@angular/animations": "~12.0.0",
        "@angular/common": "~12.0.0",
        "@angular/compiler": "~12.0.0",
        "@angular/core": "~12.0.0",
        "@angular/forms": "~12.0.0",
        "@angular/platform-browser-dynamic": "~12.0.0",
        "@angular/platform-browser": "~12.0.0",
        "@angular/router": "~12.0.0",
        "rxjs": "~6.6.0",
        "tslib": "^2.1.0",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@abp/ng.schematics": "~4.4.4"
    },
    "_devDependencies": {
        "@angular-devkit/build-angular": "~12.0.0",
        "@angular-eslint/builder": "12.1.0",
        "@angular-eslint/eslint-plugin-template": "12.1.0",
        "@angular-eslint/eslint-plugin": "12.1.0",
        "@angular-eslint/schematics": "12.1.0",
        "@angular-eslint/template-parser": "12.1.0",
        "@angular/cli": "~12.0.0",
        "@angular/compiler-cli": "~12.0.0",
        "@angular/language-service": "~12.0.4",
        "@types/jasmine": "~3.6.0",
        "@types/node": "^12.11.1",
        "@typescript-eslint/eslint-plugin": "4.23.0",
        "@typescript-eslint/parser": "4.23.0",
        "eslint": "^7.26.0",
        "jasmine-core": "~3.7.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.0.3",
        "karma-jasmine-html-reporter": "^1.5.0",
        "karma-jasmine": "~4.0.0",
        "karma": "~6.3.0",
        "ng-packagr": "^12.0.5",
        "typescript": "~4.2.3"
    },
    "tsconfig": {
        "@fs-tw/abp": [
            "libs/abp/src/index.ts"
        ],
        "@fs-tw/abp/*": [
            "libs/abp/*/src/index.ts"
        ]
    }
}