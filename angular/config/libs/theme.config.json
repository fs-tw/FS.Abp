{
  "order": 1002,
  "nx": {
    "theme": {
      "tags": []
    }
  },
  "angular": {  
    "theme": {
      "projectType": "library",
      "root": "libs/theme",
      "sourceRoot": "libs/theme/src",
      "prefix": "fs",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/theme/tsconfig.lib.json",
            "project": "libs/theme/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/theme/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/theme/tsconfig.lib.json",
              "libs/theme/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/theme/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/theme"],
          "options": {
            "jestConfig": "libs/theme/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "tsconfig": {
    "@fs-tw/theme": ["libs/theme/src/index.ts"],
    "@fs-tw/theme/*": ["libs/theme/*/src/index.ts"]
  }
}