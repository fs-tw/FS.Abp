{
  "order": 1002,
  "nx": {
    "cms": {
      "tags": []
    }
  },
  "angular": {
    "cms": {
      "projectType": "library",
      "root": "libs/cms",
      "sourceRoot": "libs/cms/src",
      "prefix": "fs",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/cms/tsconfig.lib.json",
            "project": "libs/cms/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/cms/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/cms/src/**/*.ts",
              "libs/cms/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/cms"],
          "options": {
            "jestConfig": "libs/cms/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }

  },
  "tsconfig": {
    "@fs-tw/cms": ["libs/cms/src/index.ts"],
    "@fs-tw/cms/*": ["libs/cms/*/src/index.ts"]
  }  
}