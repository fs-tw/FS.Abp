<Project Sdk="Microsoft.NET.Sdk">

  <Import Project="..\..\common.props" />

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>FS.Theme</RootNamespace>
    <GenerateEmbeddedFilesManifest>true</GenerateEmbeddedFilesManifest>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Volo.Abp.Validation" Version="4.0.2" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.FileProviders.Embedded" Version="5.0.0" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Localization\Theme\*.json" />
    <Content Remove="Localization\Theme\*.json" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\..\FS.Abp\src\FS.Abp.Domain.Shared\FS.Abp.Domain.Shared.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="DataModel\Theme.cs">
      <AutoGen>True</AutoGen>
      <DependentUpon>Theme.efml</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <None Update="DataModel\Theme.Diagram1.view">
      <AutoGen>True</AutoGen>
      <DependentUpon>Theme.efml</DependentUpon>
    </None>
    <None Update="DataModel\Theme.edps">
      <AutoGen>True</AutoGen>
      <DependentUpon>Theme.efml</DependentUpon>
    </None>
    <None Update="DataModel\Theme.efml">
      <Generator>DevartEFCoreGenerator</Generator>
      <LastGenOutput>Theme.info</LastGenOutput>
    </None>
    <None Update="DataModel\Theme.info">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Theme.efml</DependentUpon>
    </None>
  </ItemGroup>

</Project>
