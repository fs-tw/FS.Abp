    <ProjectReference Include="..\FS.Abp.Host.Shared\FS.Abp.Host.Shared.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Compile Remove="Logs\**" />
    <Content Remove="Logs\**" />
    <EmbeddedResource Remove="Logs\**" />
    <None Remove="Logs\**" />
  </ItemGroup>

  <ItemGroup>
    <Compile Include="..\FS.Abp.HttpApi.Host\Seed\AbpSampleDataSeeder.cs" Link="Seed\AbpSampleDataSeeder.cs" />
