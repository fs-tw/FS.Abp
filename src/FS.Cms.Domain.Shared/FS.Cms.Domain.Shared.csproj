<Project Sdk="Microsoft.NET.Sdk">

  <Import Project="..\..\common.props" />

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <RootNamespace>FS.Cms</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <EmbeddedResource Include="Localization\Cms\*.json" />
    <Content Remove="Localization\Cms\*.json" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="_Generated\Localization\Cms\en.json" />
    <None Remove="_Generated\Localization\Cms\zh-Hant.json" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="_Generated\Localization\Cms\en.json">
      <AutoGen>True</AutoGen>
      <DependentUpon>Cms</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Include="_Generated\Localization\Cms\zh-Hant.json">
      <AutoGen>True</AutoGen>
      <DependentUpon>Cms</DependentUpon>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Volo.Abp.Validation" Version="3.0.4" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\..\..\framework\Abp.Trees\src\EasyAbp.Abp.Trees.Domain.Shared\EasyAbp.Abp.Trees.Domain.Shared.csproj" />
    <ProjectReference Include="..\..\..\..\integrations\FS.Abp\src\FS.Abp.Domain.Shared\FS.Abp.Domain.Shared.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="DataModel\Cms.cs">
      <AutoGen>True</AutoGen>
      <DependentUpon>Cms.efml</DependentUpon>
    </Compile>
    <Compile Update="_Generated\FS.Cms\Core\FileType.Generated.cs">
      <AutoGen>True</AutoGen>
      <DependentUpon>Core</DependentUpon>
    </Compile>
    <Compile Update="_Generated\FS.Cms\DisplayMode.Generated.cs">
      <AutoGen>True</AutoGen>
      <DependentUpon>FS.Cms</DependentUpon>
    </Compile>
    <Compile Update="_Generated\Volo.Abp.Localization\LocalizationResourceExtensions.Generated.cs">
      <AutoGen>True</AutoGen>
      <DependentUpon>Volo.Abp.Localization</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <None Update="DataModel\Cms.edps">
      <AutoGen>True</AutoGen>
      <DependentUpon>Cms.efml</DependentUpon>
    </None>
    <None Update="DataModel\Cms.efml">
      <Generator>DevartEFCoreGenerator</Generator>
      <LastGenOutput>Cms.info</LastGenOutput>
    </None>
    <None Update="DataModel\Cms.info">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Cms.efml</DependentUpon>
    </None>
    <None Update="DataModel\Cms.Setting.view">
      <AutoGen>True</AutoGen>
      <DependentUpon>Cms.efml</DependentUpon>
    </None>
  </ItemGroup>

</Project>
