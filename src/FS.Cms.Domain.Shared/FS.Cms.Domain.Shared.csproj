<Project Sdk="Microsoft.NET.Sdk">

  <Import Project="..\..\common.props" />

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>FS.Cms</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <EmbeddedResource Include="Localization\Cms\*.json" />
    <Content Remove="Localization\Cms\*.json" />
  </ItemGroup>

  <ItemGroup>
    <Compile Remove="_Generated\FS.Cms\DisplayMode.Generated.cs" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="Files\Data\Codes\Definition.json" />
    <None Remove="Files\Data\Posts\Posts.json" />
    <None Remove="_Generated\Localization\Blogs\PostsPermissions\en.json" />
    <None Remove="_Generated\Localization\Blogs\PostsPermissions\zh-Hant.json" />
    <None Remove="_Generated\Localization\Core\BlogDefinitionSetting\en.json" />
    <None Remove="_Generated\Localization\Core\BlogDefinitionSetting\zh-Hant.json" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Files\Data\Codes\Definition.json" />
    <EmbeddedResource Include="Files\Data\Posts\Posts.json" />
    <EmbeddedResource Include="_Generated\Localization\Blogs\PostsPermissions\en.json">
      <AutoGen>True</AutoGen>
      <DependentUpon>PostsPermissions</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Include="_Generated\Localization\Blogs\PostsPermissions\zh-Hant.json">
      <AutoGen>True</AutoGen>
      <DependentUpon>PostsPermissions</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Include="_Generated\Localization\Core\BlogDefinitionSetting\en.json">
      <AutoGen>True</AutoGen>
      <DependentUpon>BlogDefinitionSetting</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Include="_Generated\Localization\Core\BlogDefinitionSetting\zh-Hant.json">
      <AutoGen>True</AutoGen>
      <DependentUpon>BlogDefinitionSetting</DependentUpon>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Volo.Abp.Validation" Version="3.2.1" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="DataModel\Cms.cs">
      <AutoGen>True</AutoGen>
      <DependentUpon>Cms.efml</DependentUpon>
    </Compile>
    <Compile Update="_Generated\FS.Cms\Posts\DisplayMode.Generated.cs">
      <AutoGen>True</AutoGen>
      <DependentUpon>Posts</DependentUpon>
    </Compile>
    <Compile Update="_Generated\Volo.Abp.Localization\LocalizationResourceExtensions.Generated.cs">
      <AutoGen>True</AutoGen>
      <DependentUpon>Volo.Abp.Localization</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <None Update="DataModel\Cms.edps">
      <AutoGen>True</AutoGen>
      <DependentUpon>Cms.efml</DependentUpon>
    </None>
    <None Update="DataModel\Cms.efml">
      <Generator>DevartEFCoreGenerator</Generator>
      <LastGenOutput>Cms.info</LastGenOutput>
    </None>
    <None Update="DataModel\Cms.info">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Cms.efml</DependentUpon>
    </None>
    <None Update="DataModel\Cms.Permissions.view">
      <AutoGen>True</AutoGen>
      <DependentUpon>Cms.efml</DependentUpon>
    </None>
    <None Update="DataModel\Cms.Setting.view">
      <AutoGen>True</AutoGen>
      <DependentUpon>Cms.efml</DependentUpon>
    </None>
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\..\..\framework\FS.Abp.Core\src\FS.Abp.Core.Domain.Shared\FS.Abp.Core.Domain.Shared.csproj" />
    <ProjectReference Include="..\..\..\FS.Abp\modules\FS.Abp.CodingManagement\src\FS.Abp.CodingManagement.Domain.Shared\FS.Abp.CodingManagement.Domain.Shared.csproj" />
  </ItemGroup>

</Project>
