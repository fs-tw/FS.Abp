    <ProjectReference Include="..\FS.Cms.Application.Contracts\FS.Cms.Application.Contracts.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="FS.Cms\Documents\DocumentsApi.cs">
      <AutoGen>True</AutoGen>
      <DependentUpon>Documents</DependentUpon>
    </Compile>
    <Compile Update="FS.Cms\Posts\PostsApi.cs">
      <AutoGen>True</AutoGen>
      <DependentUpon>Posts</DependentUpon>
    </Compile>
    <Compile Update="_Generated\FS.Cms\Documents\DocumentsApi.Document.Generated.cs">
      <AutoGen>True</AutoGen>
      <DependentUpon>DocumentsApi.Generated.cs</DependentUpon>
    </Compile>
    <Compile Update="_Generated\FS.Cms\Documents\DocumentsApi.DocumentDefinition.Generated.cs">
      <AutoGen>True</AutoGen>
      <DependentUpon>DocumentsApi.Generated.cs</DependentUpon>
    </Compile>
    <Compile Update="_Generated\FS.Cms\Documents\DocumentsApi.Generated.cs">
      <AutoGen>True</AutoGen>
      <DependentUpon>Documents</DependentUpon>
    </Compile>
    <Compile Update="_Generated\FS.Cms\Posts\PostsApi.Generated.cs">
      <AutoGen>True</AutoGen>
      <DependentUpon>Posts</DependentUpon>
    </Compile>
    <Compile Update="_Generated\FS.Cms\Posts\PostsApi.Post.Generated.cs">
      <AutoGen>True</AutoGen>
      <DependentUpon>PostsApi.Generated.cs</DependentUpon>
    </Compile>
    <Compile Update="_Generated\FS.Cms\Posts\PostsApi.PostTagMap.Generated.cs">
      <AutoGen>True</AutoGen>
      <DependentUpon>PostsApi.Generated.cs</DependentUpon>
    </Compile>
  </ItemGroup>

</Project>
