            <form method="get" id="DashboardFilterForm">
                <abp-row h-align="Center" v-align="Center" id="datepicker" class="mb-0 input-daterange ">
                    <div class=" col-md-auto">
                        <div class="form-group form-inline mb-3 m-md-0">
                            <input type="text"
                                   placeholder="StartDate"
                                   class="form-control"
                                   name="StartDate"
                                   value="8/15/2020" /> 
                            <input type="text"
                                   placeholder="EndDate"
                                   class="form-control ml-lg-2"
                                   name="EndDate"
                                   value="9/15/2020" />
                        </div>
                    </div>
                    <div class="col-md-auto">
                        <abp-button button-type="Primary" icon="refresh" class="btn-block" type="Submit" text="@L["Refresh"].Value" />
                    </div>
                </abp-row>
            </form>
        </abp-column>
    </abp-row> 
