<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace></RootNamespace>
    <GenerateEmbeddedFilesManifest>true</GenerateEmbeddedFilesManifest>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="FS.Abp.Domain.Shared" Version="$(FSVersion)" />
    <PackageReference Include="Volo.Abp.Validation" Version="$(AbpVersion)" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.FileProviders.Embedded" Version="$(MicrosoftPackageVersion)" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Localization\Theme\*.json" />
    <Content Remove="Localization\Theme\*.json" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\..\..\framework\FS.Abp.File\src\FS.Abp.File.Domain.Shared\FS.Abp.File.Domain.Shared.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="DataModel\Theme.cs">
      <AutoGen>True</AutoGen>
      <DependentUpon>Theme.efml</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <None Remove="Files\**\*.*" />
    <EmbeddedResource Include="Files\**\*.*" />
  </ItemGroup>

  <ItemGroup>
    <Compile Remove="_Generated\Volo.Abp.Localization\LocalizationResourceExtensions.Generated.cs" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="_Generated\Localization\Type1\en.json" />
    <None Remove="_Generated\Localization\Type1\zh-Hant.json" />
  </ItemGroup>

  <ItemGroup>
    <None Update="DataModel\Theme.Diagram1.view">
      <AutoGen>True</AutoGen>
      <DependentUpon>Theme.efml</DependentUpon>
    </None>
    <None Update="DataModel\Theme.edps">
      <AutoGen>True</AutoGen>
      <DependentUpon>Theme.efml</DependentUpon>
    </None>
    <None Update="DataModel\Theme.efml">
      <Generator>DevartEFCoreGenerator</Generator>
      <LastGenOutput>Theme.info</LastGenOutput>
    </None>
    <None Update="DataModel\Theme.info">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Theme.efml</DependentUpon>
    </None>
    <None Update="DataModel\Theme.Settings.view">
      <AutoGen>True</AutoGen>
      <DependentUpon>Theme.efml</DependentUpon>
    </None>
  </ItemGroup>

  <ItemGroup>
    <Folder Include="_Generated\Localization\Type1\" />
    <Folder Include="_Generated\Volo.Abp.Localization\" />
  </ItemGroup>

</Project>
