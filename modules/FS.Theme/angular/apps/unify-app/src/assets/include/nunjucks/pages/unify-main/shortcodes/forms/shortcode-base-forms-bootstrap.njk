                      <label for="lgFormGroupInput3" class="col-sm-2 col-form-label">Email</label>
                      <div class="col-sm-10">
                        <input type="email" class="form-control rounded-0 form-control-md" id="lgFormGroupInput3" placeholder="you@example.com">
                      </div>
                    </div>
                    <div class="form-group row">
                      <label for="smFormGroupInput" class="col-sm-2 col-form-label col-form-label-sm">Email</label>
                      <div class="col-sm-10">
                        <input type="email" class="form-control form-control-sm rounded-0 form-control-md" id="smFormGroupInput" placeholder="you@example.com">
