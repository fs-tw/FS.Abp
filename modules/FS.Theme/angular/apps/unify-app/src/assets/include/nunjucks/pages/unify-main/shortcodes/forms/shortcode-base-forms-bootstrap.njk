            <div class="col-md-10">
              <div id="shortcode5">
                <div class="shortcode-html">
                  <!-- Inline Forms (custom form controls) -->
                  <form class="g-brd-around g-brd-gray-light-v4 g-pa-30 g-mb-30">
                    <div class="form-inline">
                      <label class="mr-sm-3 mb-3 mb-lg-0" for="inlineFormCustomSelectPref">Preference</label>
                      <select class="custom-select mb-3" id="inlineFormCustomSelectPref">
                        <option selected>Choose...</option>
                        <option value="1">One</option>
                        <option value="2">Two</option>
                        <option value="3">Three</option>
                      </select>

                      <div class="custom-control custom-checkbox mr-sm-3 mb-3 mb-sm-0">
                        <input type="checkbox" class="custom-control-input" id="customControlInline">
                        <label class="custom-control-label" for="customControlInline">Remember my preference</label>
                      </div>

