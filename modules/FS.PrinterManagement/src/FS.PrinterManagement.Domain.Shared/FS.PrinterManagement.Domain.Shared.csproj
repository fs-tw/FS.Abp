		<None Update="DataModel\PrinterManagement.edps">
			<AutoGen>True</AutoGen>
			<DependentUpon>PrinterManagement.efml</DependentUpon>
		</None>
		<None Update="DataModel\PrinterManagement.efml">
			<Generator>DevartEFCoreGenerator</Generator>
			<LastGenOutput>PrinterManagement.info</LastGenOutput>
		</None>
		<None Update="DataModel\PrinterManagement.info">
			<DesignTime>True</DesignTime>
			<AutoGen>True</AutoGen>
			<DependentUpon>PrinterManagement.efml</DependentUpon>
		</None>
	</ItemGroup>

	<ItemGroup>
	  <Folder Include="_Generated\FS.PrinterManagement\Printing\" />
	</ItemGroup>

</Project>
