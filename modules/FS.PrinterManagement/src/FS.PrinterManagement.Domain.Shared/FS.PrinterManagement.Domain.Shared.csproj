	</ItemGroup>

	<ItemGroup>
		<EmbeddedResource Include="Files\**" />
	</ItemGroup>


	<ItemGroup>
		<Compile Update="DataModel\PrinterManagement.cs">
			<AutoGen>True</AutoGen>
			<DependentUpon>PrinterManagement.efml</DependentUpon>
		</Compile>
	</ItemGroup>

	<ItemGroup>
		<None Update="DataModel\PrinterManagement.Diagram1.view">
