@page
@using Microsoft.AspNetCore.Mvc.Localization
@using Volo.Abp.AspNetCore.Mvc.UI.Layout
@using Volo.Abp.AspNetCore.Mvc.UI.Theme.Shared.Demo.Views.Components.Themes.Shared.Demos.CarouselDemo
@using Volo.Abp.LeptonTheme.Demo.Localization.Resource
@using Volo.Abp.LeptonTheme.Demo.Navigation
@model Volo.Abp.LeptonTheme.Demo.Pages.Layouts.Application.Components.CarouselModel
@inject IHtmlLocalizer<DemoResource> L
@inject IPageLayout PageLayout
@{
    PageLayout.Content.Title = L["Carousel"].Value;
    PageLayout.Content.BreadCrumb.Items.Add(new BreadCrumbItem(L["BasicElements"].Value));
    PageLayout.Content.MenuItemName = DemoMenu.Components.Carousel;
}
<div class="lepton-demo">
    @await Component.InvokeAsync(typeof(CarouselDemoViewComponent))
</div> 