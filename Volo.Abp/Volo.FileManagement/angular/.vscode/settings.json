{
  "[typescriptreact]": {
    "editor.formatOnSave": false,
    "editor.codeActionsOnSave": {
      "source.fixAll": false,
      "source.organizeImports": false
    }
  },
  "[xml]": {
    "editor.formatOnSave": false,
    "editor.codeActionsOnSave": {
      "source.fixAll": false,
      "source.organizeImports": false
    }
  },
  "files.associations": {
    "*.ts.template": "typescriptreact",
    "*.html.template": "xml"
  },
  "search.exclude": {
    "**/dist": true
  },
  "tslint.rulesDirectory": "node_modules/codelyzer",
  "typescript.tsdk": "../node_modules/typescript/lib",
  "peacock.color": "#932F45",
  "workbench.colorCustomizations": {
    "activityBar.background": "#ba3b57",
    "activityBar.activeBackground": "#ba3b57",
    "activityBar.activeBorder": "#5bc23d",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#5bc23d",
    "activityBarBadge.foreground": "#15202b",
    "titleBar.activeBackground": "#932f45",
    "titleBar.inactiveBackground": "#932f4599",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveForeground": "#e7e7e799",
    "statusBar.background": "#932f45",
    "statusBarItem.hoverBackground": "#ba3b57",
    "statusBar.foreground": "#e7e7e7"
  }
}
